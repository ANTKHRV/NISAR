const [value, setValue] = useState()\r\n\r\n  const form = useRef();\r\n\r\n  const [buttonText, setButtonText] = useState(\"ПОДПИСАТЬСЯ\");\r\n\r\n  const sendEmail = (e) => {\r\n\r\n    e.preventDefault();\r\n    setButtonText(\"ЗАГРУЗКА...\");\r\n    setButtonText(\"ОТПРАВКА\");\r\n    emailjs.sendForm('service_hmoh4pe', 'template_si5n3b9', form.current, 'pBgFTDGTqgd6JkT9M')\r\n      .then((result) => {\r\n        navigate(\"/\");\r\n        // <Navigate  to={{\r\n        //  pathname: \"/\",\r\n\r\n        //  }} />;\r\n        setButtonText(\"ОТПРАВЛЕНО\");\r\n        console.log(result.text);\r\n        e.target.reset();\r\n\r\n      }, (error) => {\r\n        console.log(error.text);\r\n        setButtonText(\"ОШИБКА ОТПРАВКИ\");\r\n      });\r\n  }\r\n\r\n\r\n  return (\r\n\r\n    <div>\r\n\r\n      <div className={one.glavvvv}>\r\n        <div className={one.glavvv}>\r\n\r\n          <div className={one.glav}>\r\n\r\n            <div className={one.glav1}>\r\n              <div className={one.onas}>О нас</div>\r\n\r\n              <div className={one.onastext}>\r\n                Судоходная компания “Агентство речного флота” ведет свою деятельность с 2012 года. Одним из основных направлений деятельности является покупка/продажа судов различного назначения, реновация судов по желанию заказчика, смена типа и назначения судна с документальным сопровождением услуги или вида работ. Постоянный спрос на рынке имееют понтоны различного назначения, такие как КС63, ПМ70, ПЖ61, причалы, брандвахты, плавгостиницы, плаврестораны. На основе типовых проектов мы имеем возможность создать под ваш бизнес оптимальные конструктивные и проектные решения. Наша главная задача – создать комфортные условия работы для наших клиентов, именно поэтому мы стремимся сэкономить ваше время и деньги, предоставить наиболее широкий перечень услуг, с которыми вы можете подробно ознакомиться в разделе “Услуги”.\r\n                В нашем распоряжении находятся судна для перевозки различных видов груза (негабаритных и спецобъектов). Суда выполняют как комплексные логистические задачи компании, так и доступны для фрахтования г. Волгоград, Камышин, Астрахань, Саратов, Энгельс, Балаково, Самара, Тольятти, Сызрань, Новокуйбышевск, Дзержинск, Арзамас, Саров, Воронеж, Ростов-на-Дону, Таганрог, Шахты, Волгодонск, Новочеркасск, Санкт-Петербург, Москва, Архангельск, Северодвинск, Калининград, Мурманск, Ульяновск, Пермь, Кострома, Ярославль, Рыбинск, Череповец, Вологда,Тверь, Севастополь, Краснодар, Сочи, Казань, Набережные Челны, Ижевск, Чебоксары, Новочебоксарск, Симферополь, Ялта, Ставрополь, Пятигорск, Кисловодск, Уфа, Стерлитамак, Салават, Махачкала.\r\n                Все нюансы по транспортировке груза, в том числе сроки, порты погрузки и выгрузки, тип и общий объем груза, ставка провозной платы за единицу груза, указываются в договоре фрахтования (чартере).\r\n              </div>\r\n\r\n              <div className={one.onas2}>Подписка на нашу рассылку</div>\r\n              <div className={one.onas3}>\r\n                <div className={one.onas3text}>Вы всегда можете быть в курсе новых поступлений флота, информации о «горячих» выгодных предложениях на продажу и получать уведомления об этом на электронную почту.</div>\r\n                <div className={one.onas3form}>\r\n\r\n                  <form ref={form} onSubmit={sendEmail} >\r\n\r\n                    <input type=\"text\" title=\"Разрешено использовать только пробелы и русские буквы\"\r\n                      pattern=\"^[А-Яа-яЁё\\s]+$\" maxLength={10} name=\"us_name\" required\r\n                    />\r\n\r\n                    <input type=\"email\" name=\"email\" className={one.inp} pattern=\"^[A-Za-z0-9](([_\\.\\-]?[a-zA-Z0-9]+)*)@([A-Za-z0-9]+)(([\\.\\-]?[a-zA-Z0-9]+)*)\\.([A-Za-z])+$\" placeholder=\"Введите ваш E-mail\" maxLength={25} required\r\n                    />\r\n\r\n                    <div>\r\n                      <button type=\"submit\" className={one.onas3knop}>{buttonText}</button>\r\n                    </div>\r\n                    <ReCAPTCHA sitekey={process.env.REACT_APP_SITE_KEY} />\r\n                  </form>\r\n\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div className={one.glav2}></div>\r\n\r\n            <div className={one.glav3}>\r\n              <div className={one.glav3a}></div>\r\n            </div>\r\n\r\n          </div>\r\n\r\n\r\n\r\n\r\n        </div>\r\n\r\n\r\n      </div>\r\n\r\n    </div>\r\n\r\n\r\n  )\r\n}\r\n\r\nexport default Str2"],"mappings":";;AAAA,OAAOA,KAAK,IAAIC,QAAQ,QAAQ,OAAO;AACvC,SAASC,MAAM,QAAQ,OAAO;AAC9B,OAAOC,GAAG,MAAM,mBAAmB;AACnC,OAAO,sBAAsB;AAC7B,SAASC,WAAW,QAAQ,cAAc;AAC1C,OAAOC,OAAO,MAAM,kBAAkB;AACtC,OAAOC,SAAS,MAAM,wBAAwB;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAE/C,MAAMC,IAAI,GAAGA,CAAA,KAAM;EAAAC,EAAA;EAEjB,MAAMC,QAAQ,GAAGP,WAAW,CAAC,CAAC;EAE9B,MAAM,CAACQ,KAAK,EAAEC,QAAQ,CAAC,GAAGZ,QAAQ,CAAC,CAAC;EAEpC,MAAMa,IAAI,GAAGZ,MAAM,CAAC,CAAC;EAErB,MAAM,CAACa,UAAU,EAAEC,aAAa,CAAC,GAAGf,QAAQ,CAAC,aAAa,CAAC;EAE3D,MAAMgB,SAAS,GAAIC,CAAC,IAAK;IAEvBA,CAAC,CAACC,cAAc,CAAC,CAAC;IAClBH,aAAa,CAAC,aAAa,CAAC;IAC5BA,aAAa,CAAC,UAAU,CAAC;IACzBX,OAAO,CAACe,QAAQ,CAAC,iBAAiB,EAAE,kBAAkB,EAAEN,IAAI,CAACO,OAAO,EAAE,mBAAmB,CAAC,CACvFC,IAAI,CAAEC,MAAM,IAAK;MAChBZ,QAAQ,CAAC,GAAG,CAAC;MACb;MACA;;MAEA;MACAK,aAAa,CAAC,YAAY,CAAC;MAC3BQ,OAAO,CAACC,GAAG,CAACF,MAAM,CAACG,IAAI,CAAC;MACxBR,CAAC,CAACS,MAAM,CAACC,KAAK,CAAC,CAAC;IAElB,CAAC,EAAGC,KAAK,IAAK;MACZL,OAAO,CAACC,GAAG,CAACI,KAAK,CAACH,IAAI,CAAC;MACvBV,aAAa,CAAC,iBAAiB,CAAC;IAClC,CAAC,CAAC;EACN,CAAC;EAGD,oBAEER,OAAA;IAAAsB,QAAA,eAEEtB,OAAA;MAAKuB,SAAS,EAAE5B,GAAG,CAAC6B,OAAQ;MAAAF,QAAA,eAC1BtB,OAAA;QAAKuB,SAAS,EAAE5B,GAAG,CAAC8B,MAAO;QAAAH,QAAA,eAEzBtB,OAAA;UAAKuB,SAAS,EAAE5B,GAAG,CAAC+B,IAAK;UAAAJ,QAAA,gBAEvBtB,OAAA;YAAKuB,SAAS,EAAE5B,GAAG,CAACgC,KAAM;YAAAL,QAAA,gBACxBtB,OAAA;cAAKuB,SAAS,EAAE5B,GAAG,CAACiC,IAAK;cAAAN,QAAA,EAAC;YAAK;cAAAO,QAAA,EAAAC,YAAA;cAAAC,UAAA;cAAAC,YAAA;YAAA,OAAK,CAAC,eAErChC,OAAA;cAAKuB,SAAS,EAAE5B,GAAG,CAACsC,QAAS;cAAAX,QA